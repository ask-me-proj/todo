		"babel-loader": "^8.3.0",
		"eslint": "^8.31.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.0",
		"postcss": "^8.4.21",
		"prettier": "2.8.3",
		"pretty-quick": "^3.1.3",
		"tailwindcss": "^3.2.4",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.27.1"
	}
